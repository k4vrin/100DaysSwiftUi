+//
+//  AddLocation.swift
+//  Picbel
+//
+//  Created by Mostafa Hosseini on 7/31/23.
+//
+
+import SwiftUI
+import MapKit
+
+struct AddLocation: View {
+    var body: some View {
+        
+        @State var tracking: MapUserTrackingMode = .follow
+        @ObservedObject var managerDelegate: locationDelegate
+        @State var location: Location?
+        
+        ZStack {
+            Map(
+                coordinateRegion: $managerDelegate.region,
+                interactionModes: .all,
+                showsUserLocation: true,
+                userTrackingMode: $tracking
+            )
+            .ignoresSafeArea()
+
+            Circle()
+                .fill(.blue)
+                .opacity(0.3)
+                .frame(width: 32, height: 32)
+
+            VStack {
+                Spacer()
+
+                HStack {
+                    Spacer()
+
+                    Button {
+                        lang = managerDelegate.region.center.latitude
+                        long = managerDelegate.region.center.longitude
+                    } label: {
+                        Image(systemName: "plus")
+                            .padding()
+                            .background(.black.opacity(0.75))
+                            .foregroundColor(.white)
+                            .font(.title)
+                            .clipShape(Circle())
+                            .padding(.trailing)
+                    }
+                }
+            }
+        }
+    }
+}
+
+struct AddLocation_Previews: PreviewProvider {
+    static var previews: some View {
+        AddLocation()
+    }
+}
